@model MyCarier.Models.Social

@{
    ViewBag.Title = "Details";
    ViewBag.Header = "Details Social Url";
    ViewBag.HeaderIcon = "share-alt";
    ViewBag.NavSocials = "w3-blue";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="w3-container w3-padding-16">
    <div class="col-md-12">

        <div>
            <h4>Social</h4>
            <hr />
            <dl class="dl-horizontal">
                <dt>
                    @Html.DisplayNameFor(model => model.IconName)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.IconName)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.Url)
                </dt>

                <dd>
                    <a href="@Model.Url" target="_blank">@Model.Url</a>
                </dd>

            </dl>
        </div>
        <p>
            @Html.ActionLink("Edit", "Edit", new { id = Model.Id }, new { @class = "btn btn-warning" })
            @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-default" })
        </p>
    </div>
</div>